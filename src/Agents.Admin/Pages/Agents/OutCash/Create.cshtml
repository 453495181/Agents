@page
@model Agents.Service.Dtos.Agents.Requests.OutCashCreateRequest
@*标题*@
<page-header title="新增提现"></page-header>
@*表单*@
<util-card show-border="false">
    <util-form id="form" label-span="3" control-span="18" gutter="16">
        <util-label text="可提现余额" /> <util-label for="AbleOutMoney" />

        <util-textbox for="Money"></util-textbox>
        <util-select for="PayType" default-option-text="请选择提现方式"></util-select>
        <util-textbox for="CardId"></util-textbox>
        <util-textbox for="Note"></util-textbox>
        <util-form-item>
            <util-form-control offset="2">
                <util-button class="mr-sm" id="btnSubmit" validate-form="true" color="Primary" on-click="submit(form,btnSubmit)"></util-button>
                <util-button text="返回" on-click="back()"></util-button>
            </util-form-control>
        </util-form-item>
    </util-form>
</util-card>