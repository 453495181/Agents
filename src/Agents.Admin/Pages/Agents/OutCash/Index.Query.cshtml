@model Agents.Service.Queries.Agents.OutCashQuery

@*查询表单*@
<util-form layout="Inline" class="search__form">
    <util-row gutter="32">
        <util-column span="6">
            <util-date-picker for="BeginCreationTime" show-label="true"></util-date-picker>
        </util-column>
        <util-column span="6">
            <util-date-picker for="EndCreationTime" show-label="true"></util-date-picker>
        </util-column>
        <util-column span="6">
            <util-button id="btnQuery" text="查询" color="Primary" icon="Search" on-click="query(btnQuery)"></util-button>
            <util-button class="mx-sm" id="btnRefresh" text="刷新" icon="Sync" on-click="refresh(btnRefresh)"></util-button>
        </util-column>
    </util-row>
</util-form>