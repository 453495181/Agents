@page
@model Agents.Service.Dtos.Agents.Requests.CreateAgentRequest 
@*标题*@
<page-header title="编辑代理"></page-header>
@*表单*@
<util-card show-border="false">
    <util-form id="form" label-span="3" control-span="18" gutter="16">
        <util-textbox for="Code"></util-textbox>
        <util-textbox for="Name"></util-textbox>
        <util-textbox for="Mobile"></util-textbox>
        <util-textbox for="AlipayAccount"></util-textbox>
        <util-textbox for="WeChatAccount"></util-textbox>
        <util-textbox for="Email"></util-textbox>
        <util-textbox for="Bank"></util-textbox>
        <util-textbox for="BankUser"></util-textbox>
        <util-textbox for="BandNumber"></util-textbox>
        <util-textbox for="Qq"></util-textbox>
        <util-textbox for="Note"></util-textbox>
        <util-form-item>
            <util-form-control offset="2">
                <util-button class="mr-sm" id="btnSubmit" validate-form="true" color="Primary" on-click="submit(form,btnSubmit)"></util-button>
                <util-button text="返回" on-click="back()"></util-button>
            </util-form-control>
        </util-form-item>
    </util-form>
</util-card>