@page
@*@model Agents.Service.Dtos.Members.MemberDto*@
@model Agents.Service.Dtos.Members.Requests.MemberCreateRequest
@*标题*@
<page-header title="新增会员"></page-header>
@*表单*@
<util-card show-border="false">
    <util-form id="form" label-span="3" control-span="18" gutter="16">
        <util-textbox for="MemberOutId"></util-textbox>
        <util-textbox for="AgentId"></util-textbox>
        <util-textbox for="AgentPath"></util-textbox>
        <util-textbox for="Name"></util-textbox>
        <util-textbox for="Mobile"></util-textbox>
        <util-textbox for="IPAddress"></util-textbox>
        <util-textbox for="FirstTime"></util-textbox>
        <util-textbox for="RegisterTime"></util-textbox>
        <util-form-item>
            <util-form-control offset="2">
                <util-button class="mr-sm" id="btnSubmit" validate-form="true" color="Primary" on-click="submit(form,btnSubmit)"></util-button>
                <util-button text="返回" on-click="back()"></util-button>
            </util-form-control>
        </util-form-item>
    </util-form>
</util-card>