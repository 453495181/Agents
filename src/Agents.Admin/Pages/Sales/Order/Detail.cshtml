@page
@model Agents.Service.Dtos.Sales.OrderDto
@*标题*@
<page-header title="订单详情"></page-header>
@*表单*@
<util-card show-border="false" actions="[actionBack]">
    <util-description title="详细信息" column="3" show-border="true" >
        <util-description-item for="MemberId"></util-description-item>
        <util-description-item for="Money"></util-description-item>
        <util-description-item for="Type"></util-description-item>
        <util-description-item for="PayType"></util-description-item>
        <util-description-item for="State"></util-description-item>
        <util-description-item for="OrderTime"></util-description-item>
        <util-description-item for="PayTime"></util-description-item>
        <util-description-item for="Extend"></util-description-item>
        <util-description-item for="Note"></util-description-item>
        <util-description-item for="CreationTime"></util-description-item>
    </util-description>
    <util-template id="actionBack">
        <util-button text="返回" on-click="back()"></util-button>
    </util-template>
</util-card>