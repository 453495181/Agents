@page
@*@model Agents.Service.Dtos.Sales.OrderDto*@
@model Agents.Service.Dtos.Sales.Requests.OrderUpdateRequest
@*标题*@
<page-header title="编辑订单"></page-header>
@*表单*@
<util-card show-border="false">
    <util-form id="form" label-span="3" control-span="18" gutter="16">
        <util-textbox for="MemberId"></util-textbox>
        <util-textbox for="Money"></util-textbox>
        <util-textbox for="Type"></util-textbox>
        <util-textbox for="PayType"></util-textbox>
        <util-textbox for="State"></util-textbox>
        <util-textbox for="OrderTime"></util-textbox>
        <util-textbox for="PayTime"></util-textbox>
        <util-textbox for="Extend"></util-textbox>
        <util-textbox for="Note"></util-textbox>
        <util-form-item>
            <util-form-control offset="2">
                <util-button class="mr-sm" id="btnSubmit" validate-form="true" color="Primary" on-click="submit(form,btnSubmit)"></util-button>
                <util-button text="返回" on-click="back()"></util-button>
            </util-form-control>
        </util-form-item>
    </util-form>
</util-card>